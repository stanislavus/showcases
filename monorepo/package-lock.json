{
  "name": "monorepo",
  "version": "1.0.0",
  "lockfileVersion": 2,
  "requires": true,
  "packages": {
    "": {
      "version": "1.0.0",
      "license": "ISC",
      "workspaces": [
        "packages/*"
      ]
    },
    "node_modules/@monorepo/app": {
      "resolved": "packages/app",
      "link": true
    },
    "node_modules/@monorepo/uikit": {
      "resolved": "packages/uikit",
      "link": true
    },
    "packages/app": {
      "version": "1.0.0",
      "license": "ISC",
      "dependencies": {
        "@monorepo/uikit": "^1.0.0"
      }
    },
    "packages/uikit": {
      "version": "1.0.0",
      "license": "ISC"
    }
  },
  "dependencies": {
    "@monorepo/app": {
      "version": "file:packages/app",
      "requires": {
        "@monorepo/uikit": "^1.0.0"
      }
    },
    "@monorepo/uikit": {
      "version": "file:packages/uikit"
    }
  }
}
